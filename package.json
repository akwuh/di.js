{
  "name": "di.js",
  "version": "1.1.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel/register",
    "dev": "mocha --compilers js:babel/register -w",
    "es5": "babel index.js -s inline -m umd --module-id di -o build/di.es5.js",
    "build": "npm run es5 && uglifyjs build/di.es5.js -o build/di.es5.min.js --mangle --screw-ie8 -c join_vars,unsafe,pure_getters",
    "prepublish": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "mocha": "^2.2.5",
    "uglify-js": "^2.4.24"
  },
  "dependencies": {
    "lodash": "*"
  },
  "respository": "git@github.com:ftdebugger/di.js.git"
}
